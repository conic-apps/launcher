{
    "productName": "conic-launcher",
    "version": "0.0.0",
    "identifier": "app.conicmc.launcher",
    "build": {
        "beforeDevCommand": "pnpm dev",
        "devUrl": "http://localhost:5173",
        "beforeBuildCommand": "pnpm build",
        "frontendDist": "../dist"
    },
    "app": {
        "windows": [
            {
                "title": "Conic Launcher",
                "minWidth": 1080,
                "minHeight": 640,
                "focus": true,
                "transparent": true,
                "decorations": false,
                "fullscreen": false,
                "resizable": true,
                "shadow": true,
                "theme": "Dark"
            }
        ],
        "security": {
            "csp": null
        }
    },
    "bundle": {
        "active": true,
        "targets": "all",
        "icon": [
            "icons/32x32.png",
            "icons/128x128.png",
            "icons/128x128@2x.png",
            "icons/icon.icns",
            "icons/icon.ico"
        ],
        "createUpdaterArtifacts": true
    },
    "plugins": {
        "updater": {
            "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IDkzRkVBQTk5QThCMkQ0QwpSV1JNTFl1YXFlby9DVVo0Zi9KenYxNGI1d1o3QnRkWkJUNTc2L21TdGNiM09xcGo4VGFaVndCZgo=",
            "endpoints": [
                "https://api.conicmc.app/latest.json"
            ]
        }
    }
}
